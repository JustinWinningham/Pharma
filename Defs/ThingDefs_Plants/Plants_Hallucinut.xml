<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="PlantBase">
    <defName>Hallucinut</defName>
    <label>Hallucinut plant</label>
    <description>A nut bearing bush that contains hallucigenic properties</description>
    <statBases>
      <MaxHitPoints>70</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Hallucinut</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>50</pathCost>
    <ingestible>
      <nutrition>0.30</nutrition>
    </ingestible>
    <plant>
		<fertilitySensitivity>0.4</fertilitySensitivity>
		<growDays>12</growDays>
		<dieIfLeafless>true</dieIfLeafless>
		<reproduces>false</reproduces>
		<harvestTag>Standard</harvestTag>
		<!-- <harvestedThingDef>RawHallucinut</harvestedThingDef> -->
		<harvestYield>4</harvestYield>
		<sowMinSkill>7</sowMinSkill>
		<sowTags>
			<li>Ground</li>
			<li>Hydroponic</li>
		</sowTags>
		<topWindExposure>0.4</topWindExposure>
		<visualSizeRange>
			<min>0.85</min>
			<max>1.25</max>
		</visualSizeRange>
	</plant>
  </ThingDef>
  
</Defs>
